        "import gc\n",
        "\n",
        "def setup_llm_and_embedding():\n",
        "    \"\"\"\n",
        "    Load embedding vÃ  model LLAMA 8B, BUá»˜C Táº¢I á»Ÿ 4-bit Ä‘á»ƒ tiáº¿t kiá»‡m VRAM.\n",
        "    \"\"\"\n",
        "    global embed_model, llm\n",
        "\n",
        "    # Kiá»ƒm tra vÃ  dá»n dáº¹p bá»™ nhá»› trÆ°á»›c khi táº£i mÃ´ hÃ¬nh má»›i\n",
        "    print(\"ğŸ§¹ Äang dá»n dáº¹p VRAM trÆ°á»›c khi táº£i mÃ´ hÃ¬nh...\")\n",
        "    if 'llm' in globals():\n",
        "        del llm\n",
        "    if 'model' in globals():\n",
        "        del model\n",
        "    if 'embed_model' in globals():\n",
        "        del embed_model\n",
        "\n",
        "    gc.collect()\n",
        "    torch.cuda.empty_cache()\n",
        "    print(\"âœ… ÄÃ£ dá»n dáº¹p bá»™ nhá»› Ä‘á»‡m CUDA.\\n\")\n",
        "\n",
        "    # ============================\n",
        "    # 1. Load Embedding Model\n",
        "    # ============================\n",
        "    print(\"â³ Äang táº£i embedding...\")\n",
        "    embed_model = HuggingFaceEmbedding(model_name=\"AITeamVN/Vietnamese_Embedding\")\n",
        "    Settings.embed_model = embed_model\n",
        "    print(\"âœ… Embedding ready (VRAM chiáº¿m khoáº£ng 1-2GB).\\n\")\n",
        "\n",
        "    # ============================\n",
        "    # 2. Load Tokenizer\n",
        "    # ============================\n",
        "    LLM_MODEL_NAME = \"/content/drive/MyDrive/llama3-8b-mental-health-merged\"\n",
        "\n",
        "    print(f\"â³ Äang táº£i tokenizer cho {LLM_MODEL_NAME}...\")\n",
        "    tokenizer = AutoTokenizer.from_pretrained(LLM_MODEL_NAME, trust_remote_code=True)\n",
        "    if tokenizer.pad_token is None:\n",
        "        tokenizer.pad_token = tokenizer.eos_token\n",
        "\n",
        "    # Sá»­ dá»¥ng Chat Template máº·c Ä‘á»‹nh cá»§a Llama 3 (hoáº·c giá»¯ template custom náº¿u Ä‘Ã£ fine-tune)\n",
        "    # Giá»¯ nguyÃªn template custom cá»§a báº¡n:\n",
        "    tokenizer.chat_template = (\n",
        "        \"<|begin_of_text|>\"\n",
        "        \"{% for message in messages %}\"\n",
        "        \"{% if message['role'] == 'user' %}\"\n",
        "            \"<|start_header_id|>user<|end_header_id|>\\n\\n{{ message['content'] }}<|eot_id|>\"\n",
        "        \"{% elif message['role'] == 'system' %}\"\n",
        "            \"<|start_header_id|>system<|end_header_id|>\\n\\n{{ message['content'] }}<|eot_id|>\"\n",
        "        \"{% elif message['role'] == 'assistant' %}\"\n",
        "            \"<|start_header_id|>assistant<|end_header_id|>\\n\\n{{ message['content'] }}<|eot_id|>\"\n",
        "        \"{% endif %}\"\n",
        "        \"{% endfor %}\"\n",
        "    )\n",
        "\n",
        "    # ============================\n",
        "    # 3. Load Model VÃ€ BUá»˜C 4-bit\n",
        "    # ============================\n",
        "    print(\"âš¡ Äang táº£i Model 8B vá»›i LÆ°á»£ng tá»­ hÃ³a 4-bit báº¯t buá»™c...\")\n",
        "\n",
        "    # Cáº¥u hÃ¬nh lÆ°á»£ng tá»­ hÃ³a 4-bit (sá»­ dá»¥ng NF4 vÃ  bfloat16 cho hiá»‡u suáº¥t)\n",
        "    bnb_config = BitsAndBytesConfig(\n",
        "        load_in_4bit=True,\n",
        "        bnb_4bit_quant_type=\"nf4\",\n",
        "        bnb_4bit_compute_dtype=torch.bfloat16,\n",
        "        bnb_4bit_use_double_quant=True,\n",
        "    )\n",
        "\n",
        "    model = AutoModelForCausalLM.from_pretrained(\n",
        "        LLM_MODEL_NAME,\n",
        "        device_map=\"auto\",\n",
        "        trust_remote_code=True,\n",
        "        quantization_config=bnb_config,  # <<< BUá»˜C Sá»¬ Dá»¤NG 4-BIT\n",
        "    )\n",
        "\n",
        "    print(\"âœ… Model Llama 3 8B Ä‘Ã£ load thÃ nh cÃ´ng á»Ÿ 4-bit.\")\n",
        "    print(\"â¡ï¸ VRAM chiáº¿m dá»¥ng cho LLM sáº½ khoáº£ng 4-5GB.\")\n",
        "    print(\"â¡ï¸ Tá»•ng VRAM (LLM + Embedding) dá»± kiáº¿n: ~5-7GB.\")\n",
        "\n",
        "    # ============================\n",
        "    # 4. Wrap vá»›i HuggingFaceLLM\n",
        "    # ============================\n",
        "    eos_token_id = tokenizer.eos_token_id\n",
        "    eot_token_id = tokenizer.convert_tokens_to_ids(\"<|eot_id|>\")\n",
        "\n",
        "    llm = HuggingFaceLLM(\n",
        "        model=model,\n",
        "        tokenizer=tokenizer,\n",
        "        context_window=8192,\n",
        "        max_new_tokens=512,\n",
        "        generate_kwargs={\n",
        "            \"temperature\": 0.3,\n",
        "            \"pad_token_id\": eos_token_id\n",
        "        },\n",
        "        stopping_ids=[eos_token_id, eot_token_id],\n",
        "    )\n",
        "\n",
        "    Settings.llm = llm\n",
        "    print(\"\\nğŸ‰ Llama 3 8B 4-bit Ä‘Ã£ sáºµn sÃ ng Ä‘á»ƒ sá»­ dá»¥ng.\")"
      ]
