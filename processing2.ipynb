        "def run_full_pipeline(nodes, max_questions=100):\n",
        "    \"\"\"\n",
        "    Pipeline Ä‘Ã¡nh giÃ¡ há»‡ thá»‘ng RAG/GPT:\n",
        "    1) Sinh cÃ¢u há»i tá»« nodes\n",
        "    2) Tráº£ lá»i cÃ¢u há»i\n",
        "    3) ÄÃ¡nh giÃ¡ dá»±a trÃªn node gá»‘c\n",
        "    4) Tá»•ng há»£p káº¿t quáº£, in Ä‘iá»ƒm trung bÃ¬nh, lÆ°u CSV\n",
        "    \"\"\"\n",
        "    print(\"\\nğŸš€ Báº¯t Ä‘áº§u pipeline Ä‘Ã¡nh giÃ¡ tá»± Ä‘á»™ng...\")\n",
        "\n",
        "    # 1ï¸âƒ£ Táº¡o cÃ¢u há»i tá»« nodes\n",
        "    df_questions_all = generate_questions_from_nodes(nodes, max_q_per_node=1)\n",
        "\n",
        "    # 2ï¸âƒ£ Chá»n ngáº«u nhiÃªn max_questions cÃ¢u\n",
        "    if len(df_questions_all) > max_questions:\n",
        "        df_questions = df_questions_all.sample(n=max_questions, random_state=42).reset_index(drop=True)\n",
        "    else:\n",
        "        df_questions = df_questions_all.copy()\n",
        "\n",
        "    print(f\"ğŸ“¦ Chá»n ngáº«u nhiÃªn {len(df_questions)} cÃ¢u há»i Ä‘á»ƒ Ä‘Ã¡nh giÃ¡.\")\n",
        "\n",
        "    # 3ï¸âƒ£ Tráº£ lá»i + Ä‘Ã¡nh giÃ¡ GPT dá»±a trÃªn node gá»‘c\n",
        "    results = []\n",
        "    for idx, row in df_questions.iterrows():\n",
        "        question = row['question']\n",
        "        reference_text = row['reference_node_text']\n",
        "\n",
        "        # Tráº£ lá»i\n",
        "        answer = answer_with_gpt(question)\n",
        "\n",
        "        # ÄÃ¡nh giÃ¡ dá»±a trÃªn question, answer vÃ  reference node\n",
        "        eval_res = evaluate_with_gpt(\n",
        "            question=question,\n",
        "            answer=answer,\n",
        "            reference_text=reference_text  # <-- thÃªm tham chiáº¿u node\n",
        "        )\n",
        "\n",
        "        # LÆ°u káº¿t quáº£\n",
        "        eval_res.update({\n",
        "            \"question\": question,\n",
        "            \"answer\": answer,\n",
        "            \"reference_text\": reference_text\n",
        "        })\n",
        "        results.append(eval_res)\n",
        "\n",
        "    df_result = pd.DataFrame(results)\n",
        "\n",
        "    # 4ï¸âƒ£ In Ä‘iá»ƒm trung bÃ¬nh\n",
        "    if not df_result.empty:\n",
        "        print_average_scores(df_result)\n",
        "\n",
        "    # 5ï¸âƒ£ LÆ°u káº¿t quáº£\n",
        "    save_eval_results(df_result)\n",
        "\n",
        "    print(\"ğŸ‰ Pipeline Ä‘Ã¡nh giÃ¡ hoÃ n táº¥t!\")\n",
        "    return df_result, df_questions\n"
      ]
